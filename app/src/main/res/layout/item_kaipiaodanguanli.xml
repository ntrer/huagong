<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="@drawable/linear_border2"
    android:layout_marginStart="@dimen/dp_15"
    android:layout_marginEnd="@dimen/dp_15"
    android:layout_width="match_parent"
    android:layout_height="@dimen/dp_145">


    <RelativeLayout
        android:id="@+id/kaipiao"
        android:layout_centerHorizontal="true"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/fahuodanhao"
            android:layout_marginStart="@dimen/dp_15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="发货单号"
            android:layout_marginTop="@dimen/dp_5"
            android:layout_marginBottom="@dimen/dp_5"
            android:textSize="12sp"
            android:textColor="@color/textColor3"
            />
        <TextView
            android:layout_toRightOf="@+id/fahuodanhao"
            android:layout_centerVertical="true"
            android:layout_marginStart="@dimen/dp_10"
            android:layout_width="@dimen/dp_36"
            android:layout_height="@dimen/dp_14"
            android:gravity="center"
            android:text="未审核"
            android:textSize="@dimen/dp_9"
            android:textColor="@color/color_w"
             android:background="@drawable/textview_bg2"
            />

        <TextView
            android:layout_toRightOf="@+id/fahuodanhao"
            android:layout_centerVertical="true"
            android:layout_marginStart="@dimen/dp_10"
            android:layout_width="@dimen/dp_36"
            android:layout_height="@dimen/dp_14"
            android:gravity="center"
            android:text="已完成"
            android:textSize="@dimen/dp_9"
            android:textColor="@color/color_w"
            android:visibility="gone"
            android:background="@drawable/textview_bg3"
            />

        <ImageView
            android:layout_alignParentEnd="true"
            android:id="@+id/right_arrow"
            android:layout_width="@dimen/dp_7"
            android:layout_height="@dimen/dp_11"
            android:layout_centerVertical="true"
            android:layout_marginEnd="@dimen/dp_15"
            android:background="@mipmap/right_arrow"
            />


    </RelativeLayout>


    <View
        android:id="@+id/line"
        android:layout_below="@id/kaipiao"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:background="@drawable/shape_line_dash"
        android:layout_marginEnd="@dimen/dp_15"
        android:layout_marginStart="@dimen/dp_15"
        android:layerType="software"
        />


    <RelativeLayout
        android:id="@+id/first"
        android:layout_marginTop="@dimen/dp_3"
        android:layout_below="@+id/line"
        android:layout_centerHorizontal="true"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/xiadanriqi"
            android:layout_marginStart="@dimen/dp_15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:text="下单日期:"
            android:textSize="@dimen/sp_10"
            android:textColor="@color/textColor2"
            />

        <TextView
            android:id="@+id/xiadanriqi_text"
            android:layout_marginStart="@dimen/dp_10"
            android:layout_marginEnd="@dimen/dp_10"
            android:layout_centerVertical="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:text="2018-10-8 12:30:21"
            android:textSize="@dimen/sp_10"
            android:textColor="@color/textColor2"
            />

    </RelativeLayout>



    <RelativeLayout
        android:id="@+id/second"
        android:layout_below="@+id/first"
        android:layout_centerHorizontal="true"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">


        <TextView
        android:id="@+id/shijifachupihao"
        android:layout_marginStart="@dimen/dp_15"
        android:layout_centerVertical="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="对应实际发出生产批号"
        android:textSize="@dimen/sp_10"
        android:textColor="@color/textColor2"
        />

        <TextView
        android:id="@+id/shijifachupihao_text"
        android:layout_marginEnd="@dimen/dp_10"
        android:layout_centerVertical="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:text="123456780"
        android:textSize="@dimen/sp_10"
        android:textColor="@color/textColor2"
        />

    </RelativeLayout>


    <RelativeLayout
        android:id="@+id/third"
        android:layout_below="@+id/second"
        android:layout_centerHorizontal="true"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">


        <TextView
        android:id="@+id/gugecaigoudanhao"
        android:layout_marginTop="@dimen/dp_3"
        android:layout_marginStart="@dimen/dp_15"
        android:layout_centerVertical="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="顾客采购单号:"
        android:textSize="@dimen/sp_10"
        android:textColor="@color/textColor2"
        />

        <TextView
        android:id="@+id/gugecaigoudanhao_text"
        android:layout_marginEnd="@dimen/dp_10"
        android:layout_marginStart="@dimen/dp_10"
        android:layout_centerVertical="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:textSize="@dimen/sp_10"
        android:text="123123123230"
        android:textColor="@color/textColor2"
        />

    </RelativeLayout>


    <RelativeLayout
        android:id="@+id/four"
        android:layout_below="@+id/third"
        android:layout_centerHorizontal="true"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
        android:id="@+id/shijishuliang"
        android:layout_marginStart="@dimen/dp_15"
        android:layout_centerVertical="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="实际数量:"
        android:textSize="@dimen/sp_10"
        android:textColor="@color/textColor2"
        />

        <TextView
        android:id="@+id/shijishuliang_text"
        android:layout_marginEnd="@dimen/dp_10"
        android:layout_centerVertical="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:text="26"
        android:textSize="@dimen/sp_10"
        android:textColor="@color/textColor2"
        />

    </RelativeLayout>


    <RelativeLayout
        android:id="@+id/five"
        android:layout_below="@+id/four"
        android:layout_centerHorizontal="true"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
        android:id="@+id/baozhuang"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/dp_15"
        android:layout_centerVertical="true"
        android:text="包装:"
        android:textColor="@color/textColor2"
        android:textSize="@dimen/sp_10"/>

        <TextView
        android:id="@+id/baozhuang_text"
        android:layout_marginEnd="@dimen/dp_10"
        android:layout_marginStart="@dimen/dp_10"
        android:layout_centerVertical="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:text=""
        android:textSize="@dimen/sp_10"
        android:textColor="@color/textColor2"
        />


    </RelativeLayout>


    <RelativeLayout
        android:id="@+id/six"
        android:layout_below="@+id/five"
        android:layout_centerHorizontal="true"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/shijifahuoriqi"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/dp_15"
            android:layout_centerVertical="true"
            android:text="实际发货日期:"
            android:textColor="@color/textColor2"
            android:textSize="@dimen/sp_10"/>

        <TextView
            android:id="@+id/shijifahuoriqi_text"
            android:layout_marginEnd="@dimen/dp_10"
            android:layout_marginStart="@dimen/dp_10"
            android:layout_centerVertical="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:text="2018-10-8 12:30:21"
            android:textSize="@dimen/sp_10"
            android:textColor="@color/textColor2"
            />


    </RelativeLayout>


    <View
        android:id="@+id/line2"
        android:layout_below="@id/six"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:background="@drawable/shape_line_dash"
        android:layout_marginTop="@dimen/dp_5"
        android:layout_marginEnd="@dimen/dp_15"
        android:layout_marginStart="@dimen/dp_15"
        android:layerType="software"
        />

    <RelativeLayout
        android:id="@+id/seven"
        android:layout_below="@+id/line2"
        android:layout_marginTop="@dimen/dp_3"
        android:layout_centerVertical="true"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/zongjine"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/dp_15"
            android:layout_marginEnd="@dimen/dp_60"
            android:layout_centerVertical="true"
            android:layout_alignParentEnd="true"
            android:text="总金额:"
            android:textColor="@color/textColor4"
            android:textSize="@dimen/sp_12"/>

        <TextView
            android:id="@+id/zongjine_text"
            android:layout_marginEnd="@dimen/dp_30"
            android:layout_marginStart="@dimen/dp_10"
            android:layout_centerVertical="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:text="1000"
            android:textSize="@dimen/sp_10"
            android:textColor="@color/textColor4"
            />


    </RelativeLayout>




</RelativeLayout>