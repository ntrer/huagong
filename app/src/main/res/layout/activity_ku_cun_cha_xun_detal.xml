<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/background"
    tools:context=".activity.RestPwdActivity">

    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:background="@drawable/bg_color"
        app:titleTextColor="@android:color/white"
        android:theme="@style/ToolBarStyle"
        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:text="库存查询详情"
            android:textColor="@color/white"
            android:textSize="@dimen/sp_16"
            android:textStyle="bold"
            />

    </android.support.v7.widget.Toolbar>


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/toolbar"
        android:layout_alignParentStart="true"
        android:scrollbars="none">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <RelativeLayout
                android:orientation="vertical"
                android:background="@drawable/linear_border2"
                android:layout_width="match_parent"
                android:layout_marginTop="@dimen/dp_15"
                android:layout_marginLeft="@dimen/dp_15"
                android:layout_marginRight="@dimen/dp_15"
                android:layout_marginBottom="@dimen/dp_15"
                android:layout_height="wrap_content">

                <LinearLayout
                    android:orientation="horizontal"
                    android:id="@+id/jichuxinxi_title"
                    android:gravity="center_vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <View
                        android:layout_width="@dimen/dp_3"
                        android:layout_height="@dimen/dp_15"
                        android:background="@color/md_blue_500"
                        />

                    <TextView
                        android:id="@+id/jichuxinxi"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal"
                        android:paddingLeft="@dimen/dp_16"
                        android:layout_alignParentLeft="true"
                        android:textColor="@color/textColor"
                        android:textSize="@dimen/sp_14"
                        android:layout_marginBottom="@dimen/dp_10"
                        android:layout_marginTop="@dimen/dp_10"
                        android:text="基础信息"
                        />


                </LinearLayout>

                <View
                    android:id="@+id/line"
                    android:layout_below="@id/jichuxinxi_title"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_2"
                    android:background="@drawable/shape_line_dash"
                    android:layout_marginEnd="@dimen/dp_15"
                    android:layout_marginStart="@dimen/dp_15"
                    android:layerType="software"
                    />


                <RelativeLayout
                    android:id="@+id/renwudanhao"
                    android:layout_below="@+id/line"
                    android:orientation="horizontal"
                    android:gravity="center_vertical"
                    android:paddingLeft="@dimen/dp_16"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_weight="2"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:textColor="@color/textColor3"
                        android:textSize="@dimen/sp_12"
                        android:layout_marginBottom="@dimen/dp_10"
                        android:layout_marginTop="@dimen/dp_10"
                        android:text="任务单号"
                        />

                    <TextView
                        android:id="@+id/renwudanhao_text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:layout_alignParentEnd="true"
                        android:gravity="center"
                        android:textColor="@color/textColor3"
                        android:textSize="@dimen/sp_12"
                        android:layout_marginEnd="@dimen/dp_16"
                        android:text=""
                        />


                </RelativeLayout>


                <View
                    android:id="@+id/line_second"
                    android:layout_below="@id/renwudanhao"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_2"
                    android:background="@drawable/shape_line_dash"
                    android:layout_marginEnd="@dimen/dp_15"
                    android:layout_marginStart="@dimen/dp_15"
                    android:layerType="software"
                    />


                <RelativeLayout
                    android:id="@+id/rukuriqi"
                    android:layout_below="@+id/line_second"
                    android:orientation="horizontal"
                    android:gravity="center_vertical"
                    android:paddingLeft="@dimen/dp_16"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_weight="2"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:textColor="@color/textColor3"
                        android:textSize="@dimen/sp_12"
                        android:layout_marginBottom="@dimen/dp_10"
                        android:layout_marginTop="@dimen/dp_10"
                        android:text="入库日期"
                        />

                    <TextView
                        android:id="@+id/rukuriqi_text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:layout_alignParentEnd="true"
                        android:gravity="center"
                        android:textColor="@color/textColor3"
                        android:textSize="@dimen/sp_12"
                        android:layout_marginEnd="@dimen/dp_16"
                        android:text=""
                        />

                </RelativeLayout>




                <View
                    android:id="@+id/line_third"
                    android:layout_below="@id/rukuriqi"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_2"
                    android:background="@drawable/shape_line_dash"
                    android:layout_marginEnd="@dimen/dp_15"
                    android:layout_marginStart="@dimen/dp_15"
                    android:layerType="software"
                    />


                <RelativeLayout
                    android:id="@+id/fangongpihao"
                    android:layout_below="@+id/line_third"
                    android:orientation="horizontal"
                    android:gravity="center_vertical"
                    android:paddingLeft="@dimen/dp_16"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_weight="2"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:textColor="@color/textColor3"
                        android:textSize="@dimen/sp_12"
                        android:layout_marginBottom="@dimen/dp_10"
                        android:layout_marginTop="@dimen/dp_10"
                        android:text="返工批号"
                        />

                    <TextView
                        android:id="@+id/fangongpihao_text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:layout_alignParentEnd="true"
                        android:gravity="center"
                        android:textColor="@color/textColor3"
                        android:textSize="@dimen/sp_12"
                        android:layout_marginEnd="@dimen/dp_16"
                        android:text=""
                        />

                </RelativeLayout>



                <View
                    android:id="@+id/line_four"
                    android:layout_below="@id/fangongpihao"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_2"
                    android:background="@drawable/shape_line_dash"
                    android:layout_marginEnd="@dimen/dp_15"
                    android:layout_marginStart="@dimen/dp_15"
                    android:layerType="software"
                    />


                <RelativeLayout
                    android:id="@+id/fangongpihaoshuliang"
                    android:layout_below="@+id/line_four"
                    android:orientation="horizontal"
                    android:gravity="center_vertical"
                    android:paddingLeft="@dimen/dp_16"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_weight="2"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:textColor="@color/textColor3"
                        android:textSize="@dimen/sp_12"
                        android:layout_marginBottom="@dimen/dp_10"
                        android:layout_marginTop="@dimen/dp_10"
                        android:text="返工批号数量"
                        />

                    <TextView
                        android:id="@+id/fangongpihaoshuliang_text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:layout_alignParentEnd="true"
                        android:gravity="center"
                        android:textColor="@color/textColor3"
                        android:textSize="@dimen/sp_12"
                        android:layout_marginEnd="@dimen/dp_16"
                        android:text=""
                        />


                </RelativeLayout>

                <View
                    android:id="@+id/line_five"
                    android:layout_below="@id/fangongpihaoshuliang"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_2"
                    android:background="@drawable/shape_line_dash"
                    android:layout_marginEnd="@dimen/dp_15"
                    android:layout_marginStart="@dimen/dp_15"
                    android:layerType="software"
                    />


                <RelativeLayout
                    android:id="@+id/caozuoren"
                    android:layout_below="@+id/line_five"
                    android:orientation="horizontal"
                    android:gravity="center_vertical"
                    android:paddingLeft="@dimen/dp_16"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_weight="2"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:textColor="@color/textColor3"
                        android:textSize="@dimen/sp_12"
                        android:layout_marginBottom="@dimen/dp_10"
                        android:layout_marginTop="@dimen/dp_10"
                        android:text="操作人"
                        />

                    <TextView
                        android:id="@+id/caozuoren_text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:layout_alignParentEnd="true"
                        android:gravity="center"
                        android:textColor="@color/textColor3"
                        android:textSize="@dimen/sp_12"
                        android:layout_marginEnd="@dimen/dp_16"
                        android:text=""
                        />


                </RelativeLayout>

                <View
                    android:id="@+id/line_six"
                    android:layout_below="@id/caozuoren"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_2"
                    android:background="@drawable/shape_line_dash"
                    android:layout_marginEnd="@dimen/dp_15"
                    android:layout_marginStart="@dimen/dp_15"
                    android:layerType="software"
                    />


                <RelativeLayout
                    android:id="@+id/laiwangqingkuang"
                    android:layout_below="@+id/line_six"
                    android:orientation="horizontal"
                    android:gravity="center_vertical"
                    android:paddingLeft="@dimen/dp_16"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_weight="2"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:textColor="@color/textColor3"
                        android:textSize="@dimen/sp_12"
                        android:layout_marginBottom="@dimen/dp_10"
                        android:layout_marginTop="@dimen/dp_10"
                        android:text="来往情况"
                        />

                    <TextView
                        android:id="@+id/laiwangqingkuangtext"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:layout_alignParentEnd="true"
                        android:gravity="center"
                        android:textColor="@color/textColor3"
                        android:textSize="@dimen/sp_12"
                        android:layout_marginEnd="@dimen/dp_16"
                        android:text=""
                        />


                </RelativeLayout>




            </RelativeLayout>

            <RelativeLayout
                android:orientation="vertical"
                android:background="@drawable/linear_border2"
                android:layout_width="match_parent"
                android:layout_marginLeft="@dimen/dp_15"
                android:layout_marginRight="@dimen/dp_15"
                android:layout_marginBottom="@dimen/dp_15"
                android:layout_height="wrap_content">

                <LinearLayout
                    android:orientation="horizontal"
                    android:id="@+id/shangpin_title"
                    android:gravity="center_vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <View
                        android:layout_width="@dimen/dp_3"
                        android:layout_height="@dimen/dp_15"
                        android:background="@color/md_blue_500"
                        />

                    <TextView
                        android:id="@+id/shangpin"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal"
                        android:paddingLeft="@dimen/dp_16"
                        android:textColor="@color/textColor"
                        android:textSize="@dimen/sp_14"
                        android:layout_marginBottom="@dimen/dp_10"
                        android:layout_marginTop="@dimen/dp_10"
                        android:text="商品信息"
                        />


                </LinearLayout>

                <View
                    android:id="@+id/line2"
                    android:layout_below="@id/shangpin_title"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_2"
                    android:background="@drawable/shape_line_dash"
                    android:layout_marginEnd="@dimen/dp_15"
                    android:layout_marginStart="@dimen/dp_15"
                    android:layerType="software"
                    />

                <RelativeLayout
                    android:id="@+id/daitibaodingdan"
                    android:layout_below="@+id/line2"
                    android:orientation="horizontal"
                    android:gravity="center_vertical"
                    android:paddingLeft="@dimen/dp_16"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal"
                        android:textColor="#a26b6b6f"
                        android:textStyle="bold"
                        android:textSize="@dimen/sp_12"
                        android:layout_marginBottom="@dimen/dp_10"
                        android:layout_marginTop="@dimen/dp_10"
                        android:text="待提报订单"
                        />

                    <TextView
                        android:layout_width="@dimen/dp_12"
                        android:layout_height="@dimen/dp_12"
                        android:layout_centerInParent="true"
                        android:layout_alignParentEnd="true"
                        android:gravity="center"
                        android:textColor="@color/color_w"
                        android:background="@drawable/textview_bg"
                        android:textSize="@dimen/sp_9"
                        android:layout_marginBottom="@dimen/dp_10"
                        android:layout_marginEnd="@dimen/dp_16"
                        android:layout_marginTop="@dimen/dp_10"
                        android:text="2"
                        />


                </RelativeLayout>


            </RelativeLayout>

            <RelativeLayout
                android:orientation="vertical"
                android:background="@drawable/linear_border2"
                android:layout_width="match_parent"
                android:layout_marginLeft="@dimen/dp_15"
                android:layout_marginRight="@dimen/dp_15"
                android:layout_marginBottom="@dimen/dp_15"
                android:layout_height="wrap_content">

                <LinearLayout
                    android:orientation="horizontal"
                    android:id="@+id/beizhu_title"
                    android:gravity="center_vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <View
                        android:layout_width="@dimen/dp_3"
                        android:layout_height="@dimen/dp_15"
                        android:background="@color/md_blue_500"
                        />

                    <TextView
                        android:id="@+id/beizhu_text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal"
                        android:paddingLeft="@dimen/dp_16"
                        android:textColor="@color/textColor"
                        android:textSize="@dimen/sp_14"
                        android:layout_marginBottom="@dimen/dp_10"
                        android:layout_marginTop="@dimen/dp_10"
                        android:text="备注"
                        />


                </LinearLayout>

                <View
                    android:id="@+id/line_beizhu"
                    android:layout_below="@id/beizhu_title"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_2"
                    android:background="@drawable/shape_line_dash"
                    android:layout_marginEnd="@dimen/dp_15"
                    android:layout_marginStart="@dimen/dp_15"
                    android:layerType="software"
                    />

                <RelativeLayout
                    android:id="@+id/beizhu"
                    android:layout_below="@+id/line_beizhu"
                    android:orientation="horizontal"
                    android:gravity="center_vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/beizhu_content"
                        android:layout_width="fill_parent"
                        android:layout_height="@dimen/dp_60"
                        android:layout_margin="@dimen/dp_10"
                        android:padding="@dimen/dp_6"
                        android:enabled="true"
                        android:gravity="top"
                        android:maxLines="5"
                        android:textSize="@dimen/sp_14"
                        android:scrollbars="vertical"
                        android:background="@drawable/bg_edit_edit"
                        >
                    </TextView>


                </RelativeLayout>


            </RelativeLayout>

        </LinearLayout>


    </ScrollView>


</LinearLayout>