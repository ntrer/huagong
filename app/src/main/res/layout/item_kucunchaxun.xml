<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="@drawable/linear_border2"
    android:layout_marginStart="@dimen/dp_15"
    android:layout_marginEnd="@dimen/dp_15"
    android:layout_width="match_parent"
    android:layout_height="@dimen/dp_135">


    <RelativeLayout
        android:id="@+id/shengchan"
        android:layout_centerHorizontal="true"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/shengchanpihao"
            android:layout_marginStart="@dimen/dp_15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="生产批号"
            android:layout_marginTop="@dimen/dp_5"
            android:layout_marginBottom="@dimen/dp_5"
            android:textSize="12sp"
            android:textColor="@color/textColor3"
            />
        <!--<TextView-->
            <!--android:layout_toRightOf="@+id/shengchanpihao"-->
            <!--android:layout_centerVertical="true"-->
            <!--android:layout_marginStart="@dimen/dp_10"-->
            <!--android:layout_width="@dimen/dp_36"-->
            <!--android:layout_height="@dimen/dp_14"-->
            <!--android:gravity="center"-->
            <!--android:text="未审核"-->
            <!--android:textSize="@dimen/dp_9"-->
            <!--android:textColor="@color/color_w"-->
             <!--android:background="@drawable/textview_bg2"-->
            <!--/>-->

        <ImageView
            android:layout_alignParentEnd="true"
            android:id="@+id/right_arrow"
            android:layout_width="@dimen/dp_7"
            android:layout_height="@dimen/dp_11"
            android:layout_centerVertical="true"
            android:layout_marginEnd="@dimen/dp_15"
            android:background="@mipmap/right_arrow"
            />


    </RelativeLayout>


    <View
        android:id="@+id/line"
        android:layout_below="@id/shengchan"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:background="@drawable/shape_line_dash"
        android:layout_marginEnd="@dimen/dp_15"
        android:layout_marginStart="@dimen/dp_15"
        android:layerType="software"
        />


    <RelativeLayout
        android:id="@+id/first"
        android:layout_marginTop="@dimen/dp_3"
        android:layout_below="@+id/line"
        android:layout_centerHorizontal="true"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/guigexinghao"
            android:layout_marginStart="@dimen/dp_15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:text="规格型号:"
            android:textSize="@dimen/sp_10"
            android:textColor="@color/textColor2"
            />

        <TextView
            android:id="@+id/guigexinghao_text"
            android:layout_marginStart="@dimen/dp_10"
            android:layout_marginEnd="@dimen/dp_10"
            android:layout_centerVertical="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:text="CPC-101"
            android:textSize="@dimen/sp_10"
            android:textColor="@color/textColor2"
            />

    </RelativeLayout>



    <RelativeLayout
        android:id="@+id/second"
        android:layout_below="@+id/first"
        android:layout_centerHorizontal="true"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">


        <TextView
        android:id="@+id/bolili"
        android:layout_marginStart="@dimen/dp_15"
        android:layout_centerVertical="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="剥离力/N:"
        android:textSize="@dimen/sp_10"
        android:textColor="@color/textColor2"
        />

        <TextView
        android:id="@+id/bolili_text"
        android:layout_marginEnd="@dimen/dp_10"
        android:layout_centerVertical="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:text="4.0-4.5"
        android:textSize="@dimen/sp_10"
        android:textColor="@color/textColor2"
        />

    </RelativeLayout>


    <RelativeLayout
        android:id="@+id/third"
        android:layout_below="@+id/second"
        android:layout_centerHorizontal="true"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">


        <TextView
        android:id="@+id/naiwenxing"
        android:layout_marginTop="@dimen/dp_3"
        android:layout_marginStart="@dimen/dp_15"
        android:layout_centerVertical="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="耐温性℃:"
        android:textSize="@dimen/sp_10"
        android:textColor="@color/textColor2"
        />

        <TextView
        android:id="@+id/naiwenxing_text"
        android:layout_marginEnd="@dimen/dp_10"
        android:layout_marginStart="@dimen/dp_10"
        android:layout_centerVertical="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:textSize="@dimen/sp_10"
        android:text="230"
        android:textColor="@color/textColor2"
        />

    </RelativeLayout>


    <RelativeLayout
        android:id="@+id/four"
        android:layout_below="@+id/third"
        android:layout_centerHorizontal="true"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
        android:id="@+id/chuniangxing"
        android:layout_marginStart="@dimen/dp_15"
        android:layout_centerVertical="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="初粘性:"
        android:textSize="@dimen/sp_10"
        android:textColor="@color/textColor2"
        />

        <TextView
        android:id="@+id/chuniangxing_text"
        android:layout_marginEnd="@dimen/dp_10"
        android:layout_centerVertical="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:text="26"
        android:textSize="@dimen/sp_10"
        android:textColor="@color/textColor2"
        />

    </RelativeLayout>


    <RelativeLayout
        android:id="@+id/five"
        android:layout_below="@+id/four"
        android:layout_centerHorizontal="true"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
        android:id="@+id/fangongpihaoshuliang"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/dp_15"
        android:layout_centerVertical="true"
        android:text="返工批号数量:"
        android:textColor="@color/textColor2"
        android:textSize="@dimen/sp_10"/>

        <TextView
        android:id="@+id/fangongpihaoshuliang_text"
        android:layout_marginEnd="@dimen/dp_10"
        android:layout_marginStart="@dimen/dp_10"
        android:layout_centerVertical="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:text=""
        android:textSize="@dimen/sp_10"
        android:textColor="@color/textColor2"
        />


    </RelativeLayout>


    <RelativeLayout
        android:id="@+id/six"
        android:layout_below="@+id/five"
        android:layout_centerHorizontal="true"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/fangongpihao"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/dp_15"
            android:layout_centerVertical="true"
            android:text="返工批号:"
            android:textColor="@color/textColor2"
            android:textSize="@dimen/sp_10"/>

        <TextView
            android:id="@+id/fangongpihao_text"
            android:layout_marginEnd="@dimen/dp_10"
            android:layout_marginStart="@dimen/dp_10"
            android:layout_centerVertical="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:text=""
            android:textSize="@dimen/sp_10"
            android:textColor="@color/textColor2"
            />


    </RelativeLayout>


    <RelativeLayout
        android:id="@+id/seven"
        android:layout_below="@+id/six"
        android:layout_centerHorizontal="true"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
        android:id="@+id/rukuriqi"
        android:layout_marginStart="@dimen/dp_15"
        android:layout_centerVertical="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="入库日期:"
        android:textSize="@dimen/sp_10"
        android:textColor="@color/textColor2"
        />

        <TextView
        android:id="@+id/rukuriqi_text"
        android:layout_marginEnd="@dimen/dp_10"
        android:layout_centerVertical="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:text="2018-10-8 12:30:21"
        android:textSize="@dimen/sp_10"
        android:textColor="@color/textColor2"
        />


    </RelativeLayout>


</RelativeLayout>