<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="@drawable/linear_border2"
    android:layout_marginStart="@dimen/dp_15"
    android:layout_marginEnd="@dimen/dp_15"
    android:layout_width="match_parent"
    android:layout_height="@dimen/dp_215">


    <RelativeLayout
        android:id="@+id/shangpinming"
        android:layout_centerHorizontal="true"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/shangpinmingcheng"
            android:layout_marginStart="@dimen/dp_15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="调拨单号"
            android:layout_marginTop="@dimen/dp_5"
            android:layout_marginBottom="@dimen/dp_5"
            android:textSize="12sp"
            android:textColor="@color/md_blue_500"
            />


        <TextView
            android:id="@+id/shangpinmingcheng_text"
            android:layout_marginEnd="@dimen/dp_15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_centerVertical="true"
            android:text="允许登录"
            android:textSize="12sp"
            android:textColor="@color/textColor3"
            />

    </RelativeLayout>


    <View
        android:id="@+id/line"
        android:layout_below="@id/shangpinming"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:background="#E9EDF5"
        android:layout_marginEnd="@dimen/dp_15"
        android:layout_marginStart="@dimen/dp_15"
        />


    <RelativeLayout
        android:id="@+id/first"
        android:layout_marginTop="@dimen/dp_5"
        android:layout_below="@+id/line"
        android:layout_centerHorizontal="true"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/shengchanpihao"
            android:layout_marginStart="@dimen/dp_15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:text="生产批号"
            android:textSize="12sp"
            android:textColor="@color/textColor2"
            />

        <TextView
            android:id="@+id/shengchanpihao_text"
            android:layout_marginEnd="@dimen/dp_15"
            android:layout_centerVertical="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
           android:layout_alignParentEnd="true"
            android:text=""
            android:textSize="12sp"
            android:textColor="@color/textColor2"
            />


    </RelativeLayout>



    <RelativeLayout
        android:id="@+id/second"
        android:layout_marginTop="@dimen/dp_5"
        android:layout_below="@+id/first"
        android:layout_centerHorizontal="true"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/guige"
            android:layout_marginTop="@dimen/dp_3"
            android:layout_marginStart="@dimen/dp_15"
            android:layout_centerVertical="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="规格"
            android:textSize="12sp"
            android:textColor="@color/textColor2"
            />

        <TextView
            android:id="@+id/guige_text"
            android:layout_marginEnd="@dimen/dp_15"
            android:layout_marginStart="@dimen/dp_10"
            android:layout_centerVertical="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
           android:layout_alignParentEnd="true"
            android:text=""
            android:textSize="12sp"
            android:textColor="@color/textColor2"
            />


    </RelativeLayout>


    <RelativeLayout
        android:id="@+id/third"
        android:layout_marginTop="@dimen/dp_5"
        android:layout_below="@+id/second"
        android:layout_centerHorizontal="true"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
        android:id="@+id/shangpinshuliang"
        android:layout_marginTop="@dimen/dp_3"
        android:layout_marginStart="@dimen/dp_15"
        android:layout_centerVertical="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="商品数量:"
        android:textSize="12sp"
        android:textColor="@color/textColor2"
        />

        <TextView
        android:id="@+id/shangpinshuliang_text"
        android:layout_marginTop="@dimen/dp_3"
        android:layout_marginEnd="@dimen/dp_10"
        android:layout_centerVertical="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:text="2"
        android:textSize="12sp"
        android:textColor="@color/textColor2"
        />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/four"
        android:layout_marginTop="@dimen/dp_5"
        android:layout_below="@+id/third"
        android:layout_centerHorizontal="true"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
        android:id="@+id/bolili"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/dp_15"
        android:layout_centerVertical="true"
        android:text="剥离力"
        android:textColor="@color/textColor2"
        android:textSize="12sp"/>

        <TextView
        android:id="@+id/bolili_text"
        android:layout_marginEnd="@dimen/dp_15"
        android:layout_marginStart="@dimen/dp_10"
        android:layout_centerVertical="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:text=""
        android:textSize="12sp"
        android:textColor="@color/textColor2"
        />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/five"
        android:layout_marginTop="@dimen/dp_5"
        android:layout_below="@+id/four"
        android:layout_centerHorizontal="true"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
        android:id="@+id/naiwenxing"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_3"
        android:layout_marginStart="@dimen/dp_15"
        android:layout_centerVertical="true"
        android:text="耐温性"
        android:textColor="@color/textColor2"
        android:textSize="12sp"/>

        <TextView
        android:id="@+id/naiwenxing_text"
        android:layout_marginEnd="@dimen/dp_15"
        android:layout_marginStart="@dimen/dp_10"
        android:layout_centerVertical="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:text=""
        android:textSize="12sp"
        android:textColor="@color/textColor2"
        />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/six"
        android:layout_marginTop="@dimen/dp_5"
        android:layout_below="@+id/five"
        android:layout_centerHorizontal="true"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
        android:id="@+id/dingdanhao"
        android:layout_marginStart="@dimen/dp_15"
        android:layout_marginBottom="@dimen/dp_5"
        android:layout_centerVertical="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="订单号:"
        android:textSize="12sp"
        android:textColor="@color/textColor2"
        />

        <TextView
        android:id="@+id/dingdanhao_text"
        android:layout_marginTop="@dimen/dp_3"
        android:layout_marginEnd="@dimen/dp_10"
        android:layout_centerVertical="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:text=""
        android:textSize="12sp"
        android:textColor="@color/textColor2"
        />

    </RelativeLayout>



    <View
        android:id="@+id/line2"
        android:layout_below="@id/six"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:background="#E9EDF5"
        android:layout_marginTop="@dimen/dp_3"
        android:layout_marginEnd="@dimen/dp_15"
        android:layout_marginStart="@dimen/dp_15"
        />


    <RelativeLayout
        android:layout_marginTop="@dimen/dp_10"
        android:layout_below="@+id/line2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/shenhe"
            android:layout_marginStart="@dimen/dp_120"
            android:layout_width="@dimen/dp_38"
            android:layout_height="@dimen/dp_22"
            android:background="#CCCCCC"
            android:gravity="center"
            android:text="审核"
            android:textColor="@color/color_w"
            />


        <ImageView
            android:layout_toRightOf="@+id/shenhe"
            android:id="@+id/right_arrow"
            android:layout_width="@dimen/dp_9"
            android:layout_height="@dimen/dp_11"
            android:layout_centerVertical="true"
            android:layout_marginStart="@dimen/dp_3"
            android:layout_marginEnd="@dimen/dp_3"
            android:background="@mipmap/right_arrow"
            />

        <TextView
            android:id="@+id/chuku"
            android:layout_toRightOf="@+id/right_arrow"
            android:layout_width="@dimen/dp_38"
            android:layout_height="@dimen/dp_22"
            android:background="#CCCCCC"
            android:gravity="center"
            android:text="出库"
            android:textColor="@color/color_w"
            />


        <ImageView
            android:layout_toRightOf="@+id/chuku"
            android:id="@+id/right_arrow2"
            android:layout_width="@dimen/dp_9"
            android:layout_height="@dimen/dp_11"
            android:layout_centerVertical="true"
            android:layout_marginStart="@dimen/dp_3"
            android:background="@mipmap/right_arrow"
            />

        <TextView
            android:id="@+id/shoukuan"
            android:layout_toRightOf="@+id/right_arrow2"
            android:layout_width="@dimen/dp_38"
            android:layout_height="@dimen/dp_22"
            android:background="#CCCCCC"
            android:gravity="center"
            android:text="收款"
            android:textColor="@color/color_w"
            />


        <ImageView
            android:layout_toRightOf="@+id/shoukuan"
            android:id="@+id/right_arrow3"
            android:layout_width="@dimen/dp_9"
            android:layout_height="@dimen/dp_11"
            android:layout_centerVertical="true"
            android:layout_marginStart="@dimen/dp_3"
            android:background="@mipmap/right_arrow"
            />

        <TextView
            android:id="@+id/wancheng"
            android:layout_toRightOf="@+id/right_arrow3"
            android:layout_width="@dimen/dp_38"
            android:layout_height="@dimen/dp_22"
            android:background="#CCCCCC"
            android:gravity="center"
            android:text="完成"
            android:textColor="@color/color_w"
            />


    </RelativeLayout>




</RelativeLayout>